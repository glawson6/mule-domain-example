<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="../Header9.xsd"/>
	<xs:annotation>
		<xs:documentation>Contact Information Message contains address, phone, and url information for the specified entity.  This message will not always be accompanied by its parent Company or Individual message.  The EntityID and EntityType parameters should be used to associate the data in this message with the appropriate Company information.</xs:documentation>
	</xs:annotation>
	<xs:element name="AddressInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AddressType"/>
				<xs:element ref="AddressLine1"/>
				<xs:element ref="AddressLine2" minOccurs="0"/>
				<xs:element ref="Colony" minOccurs="0"/>
				<xs:element ref="City" minOccurs="0"/>
				<xs:element ref="State" minOccurs="0"/>
				<xs:element ref="PostalCode" minOccurs="0"/>
				<xs:element ref="Country"/>
				<xs:element ref="ReturnedMail" minOccurs="0"/>
				<xs:element ref="TitleDestination" minOccurs="0"/>
				<xs:element ref="AddressVisibility"/>
				<xs:element ref="AddressRank" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddressLine1">
		<xs:annotation>
			<xs:documentation>Address information.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AddressLine2">
		<xs:annotation>
			<xs:documentation>Additional address information.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AddressRank" type="xs:int">
		<xs:annotation>
			<xs:documentation>Indicates the address rank in priority.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AddressType">
		<xs:annotation>
			<xs:documentation>Indicates type of address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Office"/>
				<xs:enumeration value="Lot"/>
				<xs:enumeration value="Mailing"/>
				<xs:enumeration value="Home"/>
				<xs:enumeration value="Billing"/>
				<xs:enumeration value="Corporate"/>
				<xs:enumeration value="Physical"/>
				<xs:enumeration value="Bank Draft"/>
				<xs:enumeration value="Bank Reference Letter"/>
				<xs:enumeration value="Invoices"/>
				<xs:enumeration value="Arbitration"/>
				<xs:enumeration value="Payments"/>
				<xs:enumeration value="Inquiries"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AddressVisibility">
		<xs:annotation>
			<xs:documentation>Indicates permission level of who can view the address information</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Customer"/>
				<xs:enumeration value="Global"/>
				<xs:enumeration value="Group"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="City">
		<xs:annotation>
			<xs:documentation>City of address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Colony">
		<xs:annotation>
			<xs:documentation>Colony of address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ContactInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ContactType"/>
				<xs:element ref="ContactLocation"/>
				<xs:element ref="ContactValue"/>
				<xs:element ref="ContactVisibility"/>
				<xs:element ref="ContactRank" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactLocation">
		<xs:annotation>
			<xs:documentation>Indicates location of the contact</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Main"/>
				<xs:enumeration value="Corporate"/>
				<xs:enumeration value="Lot"/>
				<xs:enumeration value="Billing"/>
				<xs:enumeration value="Home"/>
				<xs:enumeration value="Physical Location"/>
				<xs:enumeration value="Mailing"/>
				<xs:enumeration value="Invoices"/>
				<xs:enumeration value="Arbitration"/>
				<xs:enumeration value="Payments"/>
				<xs:enumeration value="Inquiries"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ContactRank" type="xs:int">
		<xs:annotation>
			<xs:documentation>Indicates the contact rank in priority.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ContactType">
		<xs:annotation>
			<xs:documentation>Indicates type of contact.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Main Phone"/>
				<xs:enumeration value="Fax"/>
				<xs:enumeration value="Cell"/>
				<xs:enumeration value="Toll Free"/>
				<xs:enumeration value="Alternate"/>
				<xs:enumeration value="SMS / Text"/>
				<xs:enumeration value="Email"/>
				<xs:enumeration value="Website"/>
				<xs:enumeration value="Bank Draft"/>
				<xs:enumeration value="Bank Reference Letter"/>
				<xs:enumeration value="Arbitration"/>
				<xs:enumeration value="Online Purchasing"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ContactValue">
		<xs:annotation>
			<xs:documentation>Value of contact information</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="65"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ContactVisibility">
		<xs:annotation>
			<xs:documentation>Indicates permission level of who can view the contact information</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Customer"/>
				<xs:enumeration value="Global"/>
				<xs:enumeration value="Group"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Country">
		<xs:annotation>
			<xs:documentation>Country of address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="EntityInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="EntityID"/>
				<xs:element ref="EntityType"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EntityType">
		<xs:annotation>
			<xs:documentation>Indicates kind of entity in message.  Note: The EntityType enumerations are consistent across all messages. All values listed are relevant to this message.  Transporter is a future enhancement and will not be included until a future AuctionAccess release.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Dealership"/>
				<xs:enumeration value="Transporter"/>
				<xs:enumeration value="Commercial Account"/>
				<xs:enumeration value="Company Association"/>
				<xs:enumeration value="Person"/>
				<xs:enumeration value="Bank"/>
				<xs:enumeration value="Bank Account"/>
				<xs:enumeration value="Third Party Remarketing"/>
				<xs:enumeration value="Customer Customization"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Message">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header"/>
				<xs:element ref="EntityInformation"/>
				<xs:element ref="ContactInformation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="AddressInformation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PostalCode">
		<xs:annotation>
			<xs:documentation>Postal code of address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="20"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ReturnedMail">
		<xs:annotation>
			<xs:documentation>Indicates address has had returned mail.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="State">
		<xs:annotation>
			<xs:documentation>State/province of address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="TitleDestination">
		<xs:annotation>
			<xs:documentation>Indicates whether or not Title is to be mailed to this address.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
