<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="../Header9.xsd"/>
	<xs:annotation>
		<xs:documentation>Company Association Message contains information on an entity's related association.  Note this is an independent message from the COM message and will not always be sent with an accompanying COM message.  The CompanyID in this message should be used to associate the data in this message with the appropriate Company information.</xs:documentation>
	</xs:annotation>
	<xs:element name="Active">
		<xs:annotation>
			<xs:documentation>Indicates activity status for the Association.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AssociationID" type="xs:int">
		<xs:annotation>
			<xs:documentation>AutoTec Assocation ID</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AssociationInformation">
		<xs:annotation>
			<xs:documentation>Repeats for each association the company belongs to</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="AssociationID"/>
				<xs:element ref="AssociationName"/>
				<xs:element ref="Visibility"/>
				<xs:element ref="Active"/>
				<xs:element ref="Terminated" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AssociationName">
		<xs:annotation>
			<xs:documentation>Name of association.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CompanyID">
		<xs:annotation>
			<xs:documentation>AutoTec Company ID</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:maxInclusive value="8999999"/>
				<xs:minInclusive value="5000000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CompanyInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CompanyID"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Message">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header"/>
				<xs:element ref="CompanyInformation"/>
				<xs:element ref="AssociationInformation" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Terminated">
		<xs:annotation>
			<xs:documentation>Indicates terminated status.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Visibility">
		<xs:annotation>
			<xs:documentation>Indicates permission level of who can view the association information</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Customer"/>
				<xs:enumeration value="Global"/>
				<xs:enumeration value="Group"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
