<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="../Header9.xsd"/>
	<xs:annotation>
		<xs:documentation>Company Business Authorization Message contains the business type and dealer type information for an entity.   Note this is an independent message from the COM message and will not always be sent with an accompanying COM message.  The CompanyID in this message should be used to associate the data in this message with the appropriate Company information.</xs:documentation>
	</xs:annotation>
	<xs:element name="BusinessAuthorizationInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DealerType"/>
				<xs:element ref="New"/>
				<xs:element ref="Used"/>
				<xs:element ref="Lease"/>
				<xs:element ref="Wholesale"/>
				<xs:element ref="Salvage"/>
				<xs:element ref="Parts"/>
				<xs:element ref="Export"/>
				<xs:element ref="Rental"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CompanyID">
		<xs:annotation>
			<xs:documentation>AutoTec Company ID</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:maxInclusive value="8999999"/>
				<xs:minInclusive value="5000000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CompanyInformation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CompanyID"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DealerType">
		<xs:annotation>
			<xs:documentation>Vehicle type company deals in</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Airplanes"/>
				<xs:enumeration value="ATV"/>
				<xs:enumeration value="Automobile"/>
				<xs:enumeration value="Boats"/>
				<xs:enumeration value="Heavy Equipment"/>
				<xs:enumeration value="Mobile Home"/>
				<xs:enumeration value="Moped"/>
				<xs:enumeration value="Motorcycle"/>
				<xs:enumeration value="Motorcycle (Off Road)"/>
				<xs:enumeration value="Personal Watercraft"/>
				<xs:enumeration value="RV"/>
				<xs:enumeration value="Snowmobile"/>
				<xs:enumeration value="Trailers (Pull Behind)"/>
				<xs:enumeration value="Government Agency"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Export">
		<xs:annotation>
			<xs:documentation>Indicates business type of export.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Lease">
		<xs:annotation>
			<xs:documentation>Indicates business type of lease.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Message">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header"/>
				<xs:element ref="CompanyInformation"/>
				<xs:element ref="BusinessAuthorizationInformation" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="New">
		<xs:annotation>
			<xs:documentation>Indicates business type of new vehicles.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Parts">
		<xs:annotation>
			<xs:documentation>Indicates business type of parts.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Rental">
		<xs:annotation>
			<xs:documentation>Indicates business type of rental.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Salvage">
		<xs:annotation>
			<xs:documentation>Indicates business type of Salvage.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Used">
		<xs:annotation>
			<xs:documentation>Indicates business type of used vehicles.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Wholesale">
		<xs:annotation>
			<xs:documentation>Indicates business type of wholesale.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
