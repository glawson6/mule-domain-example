<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="../Header9.xsd"/>
	<xs:annotation>
		<xs:documentation>This message allows you to add, remove, or override certain issues.  Only Customer or Group level issues can be added or removed with this message.  Global level issues can only be overriden.  Group level issues can also be overriden in addition to added or removed.</xs:documentation>
	</xs:annotation>
	<xs:element name="Comment">
		<xs:annotation>
			<xs:documentation>Explanatory comment supplied for issue.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="150"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="DocumentInformation">
		<xs:annotation>
			<xs:documentation>This section contains document related information pertaining to a specific issue.  If no document information applies to the issue, this section is not required.  If this section does appear in the message, DocumentType should always be present.  RequiredByState and RequiredByCountry are optional fields.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DocumentType" minOccurs="0"/>
				<xs:element ref="RequiredByState" minOccurs="0"/>
				<xs:element ref="RequiredByCountry" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DocumentType">
		<xs:annotation>
			<xs:documentation>Document necessary for issue resolution.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="EffectiveDateTime">
		<xs:annotation>
			<xs:documentation>Date when issue becomes visible in AuctionACCESS and potentially begins blocking registration.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:union memberTypes="xs:dateTime">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:union>
		</xs:simpleType>
	</xs:element>
	<xs:element name="EntityInformation">
		<xs:annotation>
			<xs:documentation>Contains identification of parent entity.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="EntityID"/>
				<xs:element ref="EntityType"/>
				<xs:element ref="OverrideAll" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EntityRestriction">
		<xs:annotation>
			<xs:documentation>ID of an additional restriction to a specific entity on the Issue.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[0-9]{1,10}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="EntityType">
		<xs:annotation>
			<xs:documentation>Indicates kind of entity in message.  Note: The EntityType enumerations are consistent across all messages. The only values that pertain to this message are: Dealership, Commercial Account, and Person.  Transporter is a future enhancement and may or may not be relevant to this message in the future.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Dealership"/>
				<xs:enumeration value="Transporter"/>
				<xs:enumeration value="Commercial Account"/>
				<xs:enumeration value="Company Association"/>
				<xs:enumeration value="Person"/>
				<xs:enumeration value="Bank"/>
				<xs:enumeration value="Bank Account"/>
				<xs:enumeration value="Third Party Remarketing"/>
				<xs:enumeration value="Customer Customization"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="IssueID" type="xs:int">
		<xs:annotation>
			<xs:documentation>AutoTec Issue ID.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IssueInformation">
		<xs:annotation>
			<xs:documentation>Contains information on a specific issue.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="IssueID"/>
				<xs:element ref="IssueName"/>
				<xs:element ref="Visibility"/>
				<xs:element ref="EntityRestriction" minOccurs="0"/>
				<xs:element ref="Comment" minOccurs="0"/>
				<xs:element ref="EffectiveDateTime"/>
				<xs:element ref="DocumentInformation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Override" minOccurs="0"/>
				<xs:element ref="OverrideLevel" minOccurs="0"/>
				<xs:element ref="OverrideEntityRestriction" minOccurs="0"/>
				<xs:element ref="OverrideExpirationDate" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="IssueName">
		<xs:annotation>
			<xs:documentation>Readable description of issue.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="ADESA Document Not Scanned"/>
				<xs:enumeration value="ADESA Document Rejected"/>
				<xs:enumeration value="Admin Hold - Buy"/>
				<xs:enumeration value="Admin Hold - Sell"/>
				<xs:enumeration value="Application Not Scanned"/>
				<xs:enumeration value="Application Rejected"/>
				<xs:enumeration value="AutoTec Requested Document"/>
				<xs:enumeration value="Dealer Authorization Letter Not Scanned"/>
				<xs:enumeration value="Dealer Authorization Letter Rejected"/>
				<xs:enumeration value="Duplicate"/>
				<xs:enumeration value="Financial Hold - Buy"/>
				<xs:enumeration value="Financial Hold - Sell"/>
				<xs:enumeration value="Group Unauthorized - Buy"/>
				<xs:enumeration value="Group Unauthorized - Sell"/>
				<xs:enumeration value="ID Verification Warning"/>
				<xs:enumeration value="In Red Flags Violation"/>
				<xs:enumeration value="In Violation of AutoTec Policies"/>
				<xs:enumeration value="Individual Authorization Rejected"/>
				<xs:enumeration value="Individual Authorization Not Scanned"/>
				<xs:enumeration value="International Documentation Not Verifiable"/>
				<xs:enumeration value="Invalid Information"/>
				<xs:enumeration value="KO Book"/>
				<xs:enumeration value="License Expired"/>
				<xs:enumeration value="License Not Scanned"/>
				<xs:enumeration value="License Rejected"/>
				<xs:enumeration value="License Revoked/Cancelled/Suspended"/>
				<xs:enumeration value="Locked per Owner/Officer"/>
				<xs:enumeration value="Manheim Document Expired"/>
				<xs:enumeration value="Manheim Document Not Scanned"/>
				<xs:enumeration value="Manheim Document Rejected"/>
				<xs:enumeration value="Membership Expired"/>
				<xs:enumeration value="Missing Government ID"/>
				<xs:enumeration value="Missing Required Data"/>
				<xs:enumeration value="Owner in KO Book"/>
				<xs:enumeration value="Personal License Expired"/>
				<xs:enumeration value="Personal License Not Scanned"/>
				<xs:enumeration value="Personal License Rejected"/>
				<xs:enumeration value="Photo ID Expired"/>
				<xs:enumeration value="Photo ID Not Scanned"/>
				<xs:enumeration value="Photo ID Rejected"/>
				<xs:enumeration value="Refuses to be in Auction ACCESS"/>
				<xs:enumeration value="Representative in KO Book"/>
				<xs:enumeration value="Required Document Expired"/>
				<xs:enumeration value="Required Document Not Scanned"/>
				<xs:enumeration value="Required Document Rejected"/>
				<xs:enumeration value="Role Reinstated without Paperwork"/>
				<xs:enumeration value="Under Autotec Review"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Message">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Header"/>
				<xs:element ref="EntityInformation"/>
				<xs:element ref="IssueInformation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Override">
		<xs:annotation>
			<xs:documentation>To Add an override or resolve/update an existing override, value should be Y. To remove an override, set the OverrideExpirationDate to the current date/time.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OverrideAll">
		<xs:annotation>
			<xs:documentation>Y value indicates all global level issues that allow overrides are overridden and set to expire at midnight. This is for temporary overrides only.  The issue will reappear beginning at midnight.  N value does NOT indicate all overrides are removed. Overrides must be removed individually.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Y"/>
				<xs:enumeration value="N"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OverrideEntityRestriction">
		<xs:annotation>
			<xs:documentation>ID of an additional restriction to a specific entity on the Override</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="[0-9]{1,10}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OverrideExpirationDate">
		<xs:annotation>
			<xs:documentation>Date until which override is in effect.  An empty date means override does not expire and will remain until manually removed.  A past or current date/time will resolve the override.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:union memberTypes="xs:dateTime">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value=""/>
					</xs:restriction>
				</xs:simpleType>
			</xs:union>
		</xs:simpleType>
	</xs:element>
	<xs:element name="OverrideLevel">
		<xs:annotation>
			<xs:documentation>Issues may be overridden at the Customer or Group level.  If both override levels exist, the issue must appear twice in the message with a corresponding override level and expiration date for each.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Auction"/>
				<xs:enumeration value="Group"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RequiredByState">
		<xs:annotation>
			<xs:documentation>Issuing State for related document type.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="RequiredByCountry">
		<xs:annotation>
			<xs:documentation>Issuing Country for related document type.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="50"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Visibility">
		<xs:annotation>
			<xs:documentation>Indicates the scope of the effect of the issue</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Customer"/>
				<xs:enumeration value="Global"/>
				<xs:enumeration value="Group"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
