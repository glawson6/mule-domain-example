<xs:schema xmlns:cs="http://www.manheim.com/2009/01/CustSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.manheim.com/2009/01/CustSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="BizDemoTypes.xsd"/>
	<xs:include schemaLocation="GlobalDemoTypes.xsd"/>
	<xs:include schemaLocation="ManheimDemoTypes.xsd"/>
	<xs:complexType name="ControlAreaType">
		<xs:annotation>
			<xs:documentation>Common Header data to aid in message routing and processing.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="msgType" type="cs:msgTypeCode"/>
			<xs:element name="version" type="xs:string"/>
			<xs:element name="origSystem" type="cs:systemCode" minOccurs="0"/>
			<xs:element name="sendingSystem" type="cs:systemCode"/>
			<xs:element name="modifiedOn" type="xs:date" minOccurs="0"/>
			<xs:element name="modifiedBy" type="xs:string" minOccurs="0"/>
			<xs:element name="language" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
